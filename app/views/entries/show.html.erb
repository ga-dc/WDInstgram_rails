<div class="post">
  <img src="<%= @entry.photo_url %>">
  <p class="author">
    <%= "#{@entry.author} took this on #{@entry.date_taken}"%>
    <%= link_to "Back", entries_path %>
    <%= link_to "Edit", edit_entry_path(@entry) %>
    <%= link_to "Delete", entry_path(@entry),
                          method: :delete,
                          data: { confirm: 'Are you sure?' } %>

  </p>
</div>
