<!DOCTYPE html>
<html>
<head>
  <title>WDInstgramRails</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <h2>instagram.ish</h2>
    <h2><%= link_to "all photos", photos_path %></h2>
    <h2><%= link_to "add photo", new_photo_path %></h2>
  </nav>

  <% flash.each do |type, message| %>
    <p class="flash <%= type %>">
      <%= message %>
    </p>
  <% end %>

<%= yield %>

</body>
</html>
